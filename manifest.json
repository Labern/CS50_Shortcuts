{
  "manifest_version": 2,
  "name": "CS50 IDE",
  "description": "Provides shortcuts to Harvard's CS50x course including the edX page, lectures and problem sets.",
  "version": "1.0",

  "permissions":
  [
    "storage"
  ],

  "browser_action":
  {
    "default_icon": "icon.png",
    "default_title": "CS50 Shortcuts",
    "default_popup": "popup.html"
  },

  "icons":
  {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }

}
